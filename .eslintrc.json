{
	"env": {
		"browser": true,
		"es2021": true,
		"jest/globals": true
	},
	"extends": [
		"plugin:react/recommended",
		"plugin:jest/recommended",
		"airbnb",
		"airbnb-typescript",
		"prettier"
	],
	"parserOptions": {
		"project": "./tsconfig.json",
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "jest"],
	"rules": {}
}
